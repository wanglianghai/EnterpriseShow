<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/main_blue</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <!--实线-->
    <style name="line_tv" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:textColor">@color/black_e7e</item>
    </style>
    <style name="text" parent="android:Widget.TextView">
        <item name="android:textColor">@color/black_586</item>
        <item name="android:textColorHint">@color/black_99</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="edit" parent="android:Widget.EditText">
        <item name="android:background">@null</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColorHint">@color/black_99</item>
        <item name="android:textColor">@color/black_586</item>
    </style>

    <style name="checkBoxPicture" parent="android:Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/picture_check_box</item>
    </style>
</resources>
